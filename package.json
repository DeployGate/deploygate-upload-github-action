    "devDependencies": {
        "@types/jest": "29.5.14",
        "@types/node": "20.17.57",
        "@typescript-eslint/eslint-plugin": "5.62.0",
        "@typescript-eslint/parser": "5.62.0",
        "@vercel/ncc": "0.38.3",
        "eslint": "8.38.0",
        "jest": "29.7.0",
        "prettier": "^3.5.3",
        "ts-jest": "29.4.0",
        "typescript": "5.8.3"
    }
